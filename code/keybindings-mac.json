// Place your key bindings in this file to overwrite the defaults
[
  // Menu
  {
    "key": "cmd+s",
    "command": "workbench.action.files.saveAll"
  },
  {
    "key": "cmd+alt+s",
    "command": "workbench.action.files.save"
  },
  // Completion
  {
    "key": "alt+j",
    "command": "selectNextSuggestion",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "key": "alt+j",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "alt+k",
    "command": "selectPrevSuggestion",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "key": "alt+k",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },

  // Panels
  {
    "key": "alt+p",
    "command": "workbench.action.focusPanel"
  },

  // Editor
  // - moving left
  {
    "key": "alt+h",
    "command": "cursorLeft",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+h",
    "command": "cursorLeftSelect",
    "when": "editorTextFocus"
  },

  {
    "key": "alt+b",
    "command": "cursorHome",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+b",
    "command": "cursorHomeSelect",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+alt+h",
    "command": "cursorWordLeft",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+alt+h",
    "command": "cursorWordLeftSelect",
    "when": "editorTextFocus"
  },

  // - moving right
  {
    "key": "alt+l",
    "command": "cursorRight",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+l",
    "command": "cursorRightSelect",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+e",
    "command": "cursorEnd",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+e",
    "command": "cursorEndSelect",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+alt+l",
    "command": "cursorWordEndRight",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+shift+alt+l",
    "command": "cursorWordRightSelect",
    "when": "editorTextFocus"
  },

  // - moving down
  {
    "key": "alt+j",
    "command": "cursorDown",
    "when": "editorTextFocus && !suggestWidgetMultipleSuggestions && !suggestWidgetVisible"
  },
  {
    "key": "shift+alt+j",
    "command": "cursorDownSelect",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+d",
    "command": "cursorBottom",
    "when": "editorTextFocus"
  },

  // - moving up 
  {
    "key": "alt+k",
    "command": "cursorUp",
    "when": "editorTextFocus && !suggestWidgetMultipleSuggestions && !suggestWidgetVisible"
  },
  {
    "key": "shift+alt+k",
    "command": "cursorUpSelect",
    "when": "editorTextFocus && !suggestWidgetMultipleSuggestions && !suggestWidgetVisible"
  },
  {
    "key": "alt+u",
    "command": "cursorTop",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+u",
    "command": "cursorTopSelect",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+enter",
    "command": "-editor.action.insertLineAfter",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+b",
    "command": "-extension.vim_ctrl+b",
    "when": "editorTextFocus && vim.active && vim.use<C-b> && !inDebugRepl && vim.mode != 'Insert'"
  },
  {
    "key": "ctrl+k",
    "command": "-extension.vim_ctrl+k",
    "when": "editorTextFocus && vim.active && vim.use<C-k> && !inDebugRepl && vim.mode != 'Insert'"
  },
  {
    "key": "tab",
    "command": "-extension.vim_tab",
    "when": "editorFocus && vim.active && !inDebugRepl && vim.mode != 'Insert'"
  },
  {
    "key": "alt+space",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+space",
    "command": "-editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+space",
    "command": "toggleSuggestionDetails",
    "when": "editorTextFocus && suggestWidgetVisible"
  },
  {
    "key": "ctrl+space",
    "command": "-toggleSuggestionDetails",
    "when": "editorTextFocus && suggestWidgetVisible"
  },
  {
    "key": "ctrl+n",
    "command": "-extension.vim_ctrl+n",
    "when": "editorTextFocus && vim.active && vim.use<C-n> && !inDebugRepl"
  },
  {
    "key": "cmd+shift+k cmd+shift+right",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "cmd+alt+right",
    "command": "-workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "cmd+shift+k cmd+shift+left",
    "command": "workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "cmd+alt+left",
    "command": "-workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "cmd+k r",
    "command": "workbench.files.action.showActiveFileInExplorer"
  },
  {
    "key": "cmd+k r",
    "command": "-workbench.action.files.revealActiveFileInWindows"
  },
  {
    "key": "cmd+d",
    "command": "-list.focusPageDown",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "cmd+k cmd+j",
    "command": "workbench.action.editor.nextChange",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+f5",
    "command": "-workbench.action.editor.nextChange",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+k cmd+k",
    "command": "workbench.action.editor.previousChange",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+alt+f5",
    "command": "-workbench.action.editor.previousChange",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+j",
    "command": "-extension.vim_ctrl+j",
    "when": "editorTextFocus && vim.active && vim.use<C-j> && !inDebugRepl && vim.mode != 'Insert'"
  },
  {
    "key": "cmd+t cmd+j",
    "command": "workbench.action.terminal.focusNext"
  },
  {
    "key": "cmd+t cmd+k",
    "command": "workbench.action.terminal.focusPrevious"
  },
  {
    "key": "cmd+k o",
    "command": "workbench.action.output.toggleOutput"
  },
  {
    "key": "cmd+k cmd+h",
    "command": "-workbench.action.output.toggleOutput"
  },
  {
    "key": "cmd+g cmd+o",
    "command": "git.showOutput"
  },
  {
    "key": "ctrl+w l",
    "command": "-extension.vim_navigateRight",
    "when": "vim.active && vim.use<C-w> && !editorTextFocus"
  },
  {
    "key": "ctrl+w",
    "command": "-extension.vim_ctrl+w",
    "when": "editorTextFocus && vim.active && vim.use<C-w> && !inDebugRepl"
  },
  {
    "key": "ctrl+w j",
    "command": "-extension.vim_navigateDown",
    "when": "vim.active && vim.use<C-w> && !editorTextFocus"
  },
  {
    "key": "ctrl+w ctrl+w",
    "command": "-extension.vim_navigateGroups",
    "when": "vim.active && vim.use<C-w> && !editorTextFocus"
  },
  {
    "key": "ctrl+w h",
    "command": "-extension.vim_navigateLeft",
    "when": "vim.active && vim.use<C-w> && !editorTextFocus"
  },
  {
    "key": "ctrl+w k",
    "command": "-extension.vim_navigateUp",
    "when": "vim.active && vim.use<C-w> && !editorTextFocus"
  },
  {
    "key": "ctrl+w",
    "command": "-workbench.action.closeActiveEditor"
  },
  {
    "key": "ctrl+k w",
    "command": "-workbench.action.closeEditorsInGroup"
  },
  {
    "key": "cmd+w",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "key": "cmd+k t",
    "command": "workbench.action.terminal.focus"
  },
  {
    "key": "cmd+r",
    "command": "workbench.action.tasks.runTask"
  },
  {
    "key": "cmd+k cmd+l",
    "command": "editor.action.openLink"
  },
  {
    "key": "ctrl+d",
    "command": "-extension.vim_ctrl+d",
    "when": "editorTextFocus && vim.active && !inDebugRepl"
  },
  {
    "key": "cmd+k cmd+r",
    "command": "workbench.action.tasks.restartTask"
  },
  {
    "key": "cmd+k cmd+r",
    "command": "-workbench.action.keybindingsReference"
  },

  // Git
  {
    "key": "cmd+g cmd+c",
    "command": "git.checkout"
  },
  {
    "key": "cmd+g cmd+p",
    "command": "git.push"
  },
  {
    "key": "cmd+g p",
    "command": "git.pull"
  },
  {
    "key": "cmd+g c",
    "command": "git.commit"
  },
  {
    "key": "cmd+g cmd+m",
    "command": "git.merge"
  },
  {
    "key": "cmd+g s",
    "command": "git.stage"
  },

  // Compare editor
  {
    "key": "cmd+k",
    "command": "workbench.action.compareEditor.previousChange",
    "when": "isInDiffEditor"
  },
  {
    "key": "cmd+j",
    "command": "workbench.action.compareEditor.nextChange",
    "when": "isInDiffEditor"
  },
  {
    "key": "ctrl+f",
    "command": "-extension.vim_ctrl+f",
    "when": "editorTextFocus && vim.active && vim.use<C-f> && !inDebugRepl"
  }
]