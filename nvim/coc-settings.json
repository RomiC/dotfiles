{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "javascriptreact",
    "less",
    "markdown",
    "Markdown",
    "typescript",
    "typescript.tsx",
    "typescriptreact"
  ],

  "eslint.enable": true,
  "eslint.filetypes": ["javascript", "typescript", "typescriptreact", "javascriptreact", "typescript.tsx"],
  "eslint.autoFixOnSave": true,

  "javascript.showUnused": false,
  "javascript.format.enabled": false,

  "prettier.requireConfig": true,
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,

  "typescript.format.enabled": false,
  "typescript.showUnused": false
}
